<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.example.dao.DividendDao">
    <select id="getDividends" parameterType="DividendDto" resultType="com.example.dto.DividendDto">
        SELECT ric, tkr, rec_dt, py_dt, dvd_r, DVD_CURR
        FROM TMTSKR843I00
        WHERE TKR = #{tkr}
          AND dvd_r > 0
        ORDER BY REC_DT DESC limit 10;
    </select>

</mapper>